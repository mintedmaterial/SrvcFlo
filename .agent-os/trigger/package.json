{
  "name": "serviceflow-ai-trigger-orchestration",
  "version": "0.1.0",
  "private": true,
  "description": "Trigger.dev orchestration layer for ServiceFlow AI agents",
  "scripts": {
    "dev": "npx trigger.dev@latest dev",
    "deploy": "npx trigger.dev@latest deploy",
    "deploy:staging": "npx trigger.dev@latest deploy --env staging",
    "deploy:production": "npx trigger.dev@latest deploy --env production",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@trigger.dev/sdk": "^3.0.0",
    "@trigger.dev/react-hooks": "^3.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}