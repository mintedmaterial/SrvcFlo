{
  "name": "serviceflow-ai-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:static": "move app\\api api.temp && next build && move api.temp app\\api",
    "start": "next start",
    "lint": "next lint",
    "deploy:worker": "npm run build:static && npx wrangler deploy",
    "dev:worker": "wrangler dev",
    "dev:inft": "wrangler dev --local --config wrangler.toml",
    "build:inft": "tsc --project tsconfig.workers.json --noEmit && wrangler deploy --dry-run",
    "deploy:staging": "wrangler deploy --env staging",
    "deploy:production": "wrangler deploy --env production",
    "setup:durable-objects": "wrangler d1 migrations apply DB --env staging",
    "contracts:compile": "npx hardhat compile",
    "contracts:check": "npx hardhat run scripts/pre-deployment-check.js --network sonic",
    "contracts:deploy": "npx hardhat run scripts/deploy-mainnet-final.js --network sonic",
    "contracts:verify": "npx hardhat run scripts/verify-mainnet-contracts.js --network sonic",
    "contracts:deploy-all": "npx hardhat run scripts/deploy-and-verify.js --network sonic",
    "contracts:testnet": "npx hardhat run scripts/deploy-sonic-contracts.js --network sonicTestnet",
    "contracts:credits": "npx hardhat run scripts/deploy-credit-system.js --network sonicTestnet",
    "contracts:erc7857-testnet": "npx hardhat run scripts/deploy-erc7857-testnet.js --network sonicTestnet",
    "contracts:erc7857-mainnet": "npx hardhat run scripts/deploy-erc7857-mainnet.js --network sonic",
    "mint:orchestrator-inft": "npx hardhat run scripts/mint-orchestrator-inft.js --network sonic",
    "update:token1-flo": "npx hardhat run scripts/update-token1-flo.js --network sonic"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.66",
    "@google/genai": "^1.12.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@openzeppelin/contracts-upgradeable": "^5.4.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.83.0",
    "@web3auth/modal": "^10.0.4",
    "agents": "^0.0.109",
    "ai": "^3.4.32",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "ethers": "^6.8.0",
    "express": "^5.1.0",
    "lucide-react": "^0.454.0",
    "mongodb": "^6.3.0",
    "next": "^14.0.4",
    "next-themes": "^0.2.1",
    "openai": "^4.75.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "thirdweb": "^5.77.0",
    "viem": "^2.33.1",
    "wagmi": "^2.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.6",
    "@cloudflare/workers-types": "^4.20241218.0",
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@types/node": "^20",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5",
    "wrangler": "^4.25.0"
  }
}
